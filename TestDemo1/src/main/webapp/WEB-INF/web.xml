<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    
    <servlet>
        <servlet-name>HelloServlet2</servlet-name>
        <servlet-class>dev.xuanran.TestDemo1.HelloServlet2</servlet-class>
        <init-param>
            <param-name>username</param-name>
            <param-value>XuanRanDev</param-value>
        </init-param>
    </servlet>
    <context-param>
        <param-name>Blog</param-name>
        <param-value>https://xuanran.cc</param-value>
    </context-param>
    <servlet-mapping>
        <servlet-name>HelloServlet2</servlet-name>
        <url-pattern>/helloServlet2</url-pattern>
    </servlet-mapping>


    <servlet>
        <servlet-name>HelloServlet3</servlet-name>
        <servlet-class>dev.xuanran.TestDemo1.HelloServlet3</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>HelloServlet3</servlet-name>
        <url-pattern>/helloServlet3</url-pattern>
    </servlet-mapping>
</web-app>